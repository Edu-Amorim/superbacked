{
  "name": "superbacked",
  "productName": "Superbacked",
  "version": "1.8.0",
  "description": "Superbacked is a secret management platform used to back up and pass on sensitive data from one generation to the next.",
  "main": ".webpack/main",
  "scripts": {
    "code": "export ENV=development; electron-forge start",
    "build": "export ENV=production; electron-forge make && electron-builder --config ./builder.config.js --linux --mac",
    "build:staging": "export ENV=production STAGING=true; electron-forge make && electron-builder --config ./builder.config.js --mac",
    "sign-dependencies": "find ./{bin,superbacked-os/*.img,superbacked-os-utilities/sedutil-cli} -type f -print0 | sort -z | xargs -0 shasum --algorithm 256 > SHA256SUMS; gpg --armor --detach-sig --output SHA256SUMS.asc SHA256SUMS",
    "sign-release": "cd dist; find . -name 'superbacked-*' -type f -print0 | sort -z | xargs -0 shasum --algorithm 256 > SHA256SUMS; gpg --armor --detach-sig --output SHA256SUMS.asc SHA256SUMS; cd -",
    "lint": "eslint --ext .ts,.tsx --format=visualstudio .",
    "ncu": "ncu --reject @vercel/webpack-asset-relocator-loader --target minor --upgrade",
    "package": "./package.sh"
  },
  "keywords": [],
  "author": {
    "name": "Sun Knudsen",
    "email": "hello@sunknudsen.com"
  },
  "license": "UNLICENSED",
  "private": true,
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-webpack": "^7.8.1",
    "@electron/notarize": "^2.5.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.0",
    "@svgr/webpack": "^8.1.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "css-loader": "^6.11.0",
    "dotenv-webpack": "^8.1.0",
    "electron": "^28.3.3",
    "electron-builder": "^26.0.17",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "fs-extra": "^11.3.0",
    "node-loader": "^2.1.0",
    "npm-check-updates": "^16.14.20",
    "react-refresh": "^0.17.0",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.2",
    "typescript": "~5.8.3"
  },
  "dependencies": {
    "@fontsource/roboto-mono": "^5.2.6",
    "@mantine/core": "^5.10.5",
    "@mantine/form": "^5.10.5",
    "@mantine/hooks": "^5.10.5",
    "@sunknudsen/totp": "^1.2.0",
    "@zxcvbn-ts/core": "^3.0.4",
    "@zxcvbn-ts/language-common": "^3.0.4",
    "@zxcvbn-ts/language-en": "^3.0.2",
    "bip39": "^3.1.0",
    "blockcrypt": "^0.0.1-beta.22",
    "commander": "^11.1.0",
    "electron-store": "^8.2.0",
    "got": "^12.6.1",
    "jsqr": "^1.4.0",
    "leven": "^4.0.0",
    "pdfjs-dist": "^4.10.38",
    "polished": "^4.3.1",
    "prettier": "^3.5.3",
    "qrcode.react": "^4.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^11.18.6",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19",
    "tabler-icons-react": "^1.56.0",
    "zod": "^3.25.67"
  },
  "prettier": {
    "endOfLine": "lf",
    "printWidth": 80,
    "semi": false,
    "tabWidth": 2,
    "trailingComma": "es5"
  }
}
